<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="checkoServicesPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>br.com.checko.services.TbEstados</class>
    <class>br.com.checko.services.TbPercurso</class>
    <class>br.com.checko.services.TbCidades</class>
    <class>br.com.checko.services.TbClube</class>
    <class>br.com.checko.services.TbGruposUsuarios</class>
    <class>br.com.checko.services.TbInscricao</class>
    <class>br.com.checko.services.TbPenalidade</class>
    <class>br.com.checko.services.TbAtleta</class>
    <class>br.com.checko.services.TbResultadoProva</class>
    <class>br.com.checko.services.TbProva</class>
    <class>br.com.checko.services.TbCategoria</class>
    <class>br.com.checko.services.TbPrisma</class>
    <class>br.com.checko.services.TbEvento</class>
    <class>br.com.checko.services.TbUsuarios</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/db_check_o?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
